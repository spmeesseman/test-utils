{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
		"baseUrl": ".",
        "esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"incremental": true,
		"isolatedModules": true,
		"lib": [ "es2020" ],
        "module": "commonjs",
		"moduleResolution": "node",
		"noImplicitOverride": true,
        "noUnusedLocals": false,
        "noEmitOnError": true,
		"skipLibCheck": true,
        "sourceMap": true,
        "strict": true,
        "target": "es2020",
		"useDefineForClassFields": true,
		"useUnknownInCatchVariables": false,
        "noEmit": true,
        "rootDir": "src",
        "tsBuildInfoFile": "./node_modules/.cache/tsconfig.example.tsbuildinfo",
        "paths": {
			"@spmeesseman/test-utils": [ "../.." ],
            "@vscode/test-electron": [ "../../../../vscode-taskexplorer/node_modules/@vscode/test-electron" ],
            "vscode": [ "../../../../vscode-taskexplorer/node_modules/vscode" ]
		}
    },
    "include": [
        "src"
    ],
    "exclude": [
        "src/test"
    ]
}